{
	"id": "nodebb-plugin-sso-twt",
	"name": "NodeBB TwT SSO",
	"description": "NodeBB Plugin that allows users to login/register via TwT SSO.",
	"url": "https://github.com/zhyupe/nodebb-plugin-sso-twt",
	"library": "./library.js",
	"hooks": [
		{ "hook": "static:user.delete", "method": "deleteUserData" },
		{ "hook": "filter:auth.init", "method": "getStrategy" }
	]
}
